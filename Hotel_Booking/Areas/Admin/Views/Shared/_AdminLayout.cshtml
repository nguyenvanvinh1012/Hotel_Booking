

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <title>@ViewData["Title"]</title>
    @Scripts.Render("~/bundles/modernizr")

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/assets/images/favicon.png">
    <link rel="stylesheet" href="~/Content/Adminassets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/Adminassets/plugins/fontawesome/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/Content/Adminassets/plugins/fontawesome/css/all.min.css">
    <link rel="stylesheet" href="~/Content/Adminassets/css/feathericon.min.css">
    <link rel="stylehseet" href="https://cdn.oesmith.co.uk/morris-0.5.1.css">
    <link rel="stylesheet" href="~/Content/Adminassets/plugins/morris/morris.css">
    <link rel="stylesheet" href="~/Content/Adminassets/css/style.css">
</head>

<body>
    <div class="main-wrapper">
        <div class="header">
            <div class="header-left">
                @*<img src="~/Content/assets/images/logo.png" alt="image">*@
                <a href="index.html" class="logo"> <img src="~/Content/assets/images/logo.png" width="50" height="70" alt="logo"> <span class="logoclass"></span> </a>
                <a href="index.html" class="logo logo-small"> <img src="~/Content/assets/images/favicon.png" alt="Logo" width="30" height="30"> </a>
            </div>
            <a href="javascript:void(0);" id="toggle_btn"> <i class="fe fe-text-align-left"></i> </a>
            <a class="mobile_btn" id="mobile_btn"> <i class="fas fa-bars"></i> </a>
            <ul class="nav user-menu">
                <li class="nav-item dropdown noti-dropdown">
                    <a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown"> <i class="fe fe-bell"></i> <span class="badge badge-pill">3</span> </a>
                    <div class="dropdown-menu notifications">
                        <div class="topnav-dropdown-header"> <span class="notification-title">Notifications</span> <a href="javascript:void(0)" class="clear-noti"> Clear All </a> </div>
                        <div class="noti-content">
                            <ul class="notification-list">
                               
                            </ul>
                        </div>
                        <div class="topnav-dropdown-footer"> <a href="#">View all Notifications</a> </div>
                    </div>
                </li>
                <li class="nav-item dropdown has-arrow">
                    <a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown"> <span class="user-img"><img class="rounded-circle" src="~/Content/Adminassets/img/vanvinh.jpg" width="31" alt="Soeng Souy"></span> </a>
                    <div class="dropdown-menu">
                        <div class="user-header">
                            <div class="avatar avatar-sm"> <img src="" alt="User Image" class="avatar-img rounded-circle"> </div>
                            <div class="user-text">
                                <h6>Soeng Souy</h6>
                                <p class="text-muted mb-0">Administrator</p>
                            </div>
                        </div> <a class="dropdown-item" href="profile.html">My Profile</a> <a class="dropdown-item" href="settings.html">Account Settings</a> <a class="dropdown-item" href="login.html">Logout</a>
                    </div>
                </li>
            </ul>
            <div class="top-nav-search">
                <form>
                    <input type="text" class="form-control" placeholder="Search here">
                    <button class="btn" type="submit"><i class="fas fa-search"></i></button>
                </form>
            </div>
        </div>

        @Html.Partial("_SideNavPartialView")

        <div class="page-wrapper">
            @RenderBody()
        
        </div>
    </div>




    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)

    @*<script data-cfasync="false" src="../../../cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>*@
    <script src="~/Content/Adminassets/js/jquery-3.5.1.min.js"></script>
    <script src="~/Content/Adminassets/js/popper.min.js"></script>
    <script src="~/Content/Adminassets/js/bootstrap.min.js"></script>
    <script src="~/Content/Adminassets/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Content/Adminassets/plugins/raphael/raphael.min.js"></script>
    <script src="~/Content/Adminassets/plugins/morris/morris.min.js"></script>
    <script src="~/Content/Adminassets/js/chart.morris.js"></script>
    <script src="~/Content/Adminassets/js/script.js"></script>
</body>

</html>